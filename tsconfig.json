{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/Login.js"
, "app/_layout.js"  ],
  "allowImportingTsExtensions": true,
  "moduleResolution": "bundler",
  "noEmit": true
}